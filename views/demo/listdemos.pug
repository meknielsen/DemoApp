.container
  #accordion.panel-group
    - var i = 0;
    each demo, index in demos
     - ++i;
     .panel.panel-default
      .panel-heading
       h4.panel-title
        a(data-toggle='collapse', data-parent='#accordion', href=`#collapse${i}`, class='collapsed')
        = demo.station
      .panel-collapse.collapse.in(id=`collapse${i}`)
        .panel-body
         | test
      